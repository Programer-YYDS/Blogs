域-->\r\n          <div class=\"ui top attached segment\">\r\n            <div class=\"ui middle aligned two column grid\">\r\n              <div class=\"column\">\r\n                <h3 class=\"ui teal header\">博客列表</h3>\r\n              </div>\r\n              <div class=\"right aligned column\">\r\n                共 <h2 class=\"ui orange header m-inline-block m-text-thin\" th:text=\"${page.totalElements}\"></h2> 篇\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!--左侧博客列表-->\r\n          <div class=\"ui attached  segment\">\r\n\r\n            <div class=\"ui padded vertical segment m-padded-tb-large\" th:each=\"blog : ${page.content}\">\r\n              <div class=\"ui middle aligned mobile reversed stackable grid\" >\r\n                <div class=\"eleven wide column\">\r\n                  <h3 class=\"ui header\" ><a href=\"#\" th:href=\"@{/blog/{id}(id=${blog.id})}\" target=\"_blank\" class=\"m-black\" th:text=\"${blog.title}\">?</a></h3>\r\n                  <p  th:text=\"|${blog.description}......|\"></p>\r\n                  <div class=\"ui grid\">\r\n                    <div class=\"eleven wide column\">\r\n                      <div class=\"ui mini horizontal link list\">\r\n                        <div class=\"item\">\r\n                          <a target=\"_blank\" th:href=\"@{/about}\"> <img src=\"https://unsplash.it/100/100?image=1005\" th:src=\"@{${blog.user.avatar}}\"  alt=\"\" class=\"ui avatar image\"> </a>\r\n                        </div>\r\n                        <div class=\"item\">\r\n                          <div class=\"content\"><h4><a th:href=\"@{/about}\" target=\"_blank\" class=\"header\" th:text=\"${blog.user.nickname}\" ></a></h4></div>\r\n                        </div>\r\n                        <div class=\"item\">\r\n                          <h4 th:text=\"${#dates.format(blog.createTime,'yyyy-MM-dd')}\"></h4>\r\n                        </div>\r\n                        <div class=\"item\">\r\n 